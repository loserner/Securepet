{
  "name": "pet-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "server": "ts-node src/server.ts",
    "server:watch": "nodemon --watch src --ext ts --exec \"ts-node src/server.ts\"",
    "chain": "hardhat node",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.ts --network localhost",
    "deploy:sepolia": "hardhat run scripts/deploy.ts --network sepolia"
  },
  "dependencies": {
    "@fhevm/solidity": "^0.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@fhevm/hardhat-plugin": "0.1.0",
    "@openzeppelin/contracts": "^5.0.2",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "hardhat": "^2.26.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  }
}


